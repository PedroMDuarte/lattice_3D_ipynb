Cell["\<\
Lattice depth and potential envelopes\
\>", "Subsubsection",
 CellChangeTimes->{{3.604800486181453*^9, 3.604800492178775*^9}, 
   3.604971712088418*^9}],

Cell["As a function of distance along 111 the lattice depth is ", "Text",
 CellChangeTimes->{{3.604799157818593*^9, 3.604799170710371*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"vL111", "[", " ", 
   RowBox[{"r_", ",", "s0_", ",", "w_"}], "]"}], ":=", 
  RowBox[{"s0", " ", 
   RowBox[{"Exp", "[", 
    RowBox[{"-", " ", 
     FractionBox[
      RowBox[{"4", " ", 
       SuperscriptBox["r", "2"]}], 
      RowBox[{"3", " ", 
       SuperscriptBox["w", "2"]}]]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.604799171875977*^9, 3.604799222110544*^9}, {
  3.60480052331436*^9, 3.604800525357142*^9}, {3.604800562491087*^9, 
  3.604800563690695*^9}}],

Cell["This is also the envelope for the bottom of the potential", "Text",
 CellChangeTimes->{{3.604799241619723*^9, 3.60479928553754*^9}}],

Cell["\<\
For the compensationg potential we have  the same equation\
\>", "Text",
 CellChangeTimes->{{3.60497171554844*^9, 3.604971730804113*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"vC111", "[", 
   RowBox[{"r_", ",", "g0_", ",", "w_"}], "]"}], ":=", 
  RowBox[{"g0", " ", 
   RowBox[{"Exp", "[", 
    RowBox[{"-", " ", 
     FractionBox[
      RowBox[{"4", 
       SuperscriptBox["r", "2"]}], 
      RowBox[{"3", " ", 
       SuperscriptBox["w", "2"]}]]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.604971732365452*^9, 3.604971756346489*^9}}]
}, Open  ]],

